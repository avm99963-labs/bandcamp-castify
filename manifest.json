{
  "manifest_version": 2,
  "name": "Castify",
  "version": "0.1",
  "description": "Castify websites",
  "permissions": [
    "https://*.bandcamp.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.bandcamp.com/track/*"],
      "js": ["bandcamp.js"]
    }
  ],
  "web_accessible_resources": ["bandcamp_actual.js"]
}
